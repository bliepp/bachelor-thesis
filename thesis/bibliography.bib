@Article{song2012effects,
  author     = {Song, Bo and Dong, Shujuan and Zhang, Baicheng and Liao, Hanlin and Coddet, Christian},
  journal    = {Materials \& Design},
  title      = {Effects of processing parameters on microstructure and mechanical property of selective laser melted {Ti6Al4V}},
  year       = {2012},
  issn       = {0261-3069},
  month      = mar,
  pages      = {120--125},
  volume     = {35},
  abstract   = {Selective laser melting, as a facile method, was successfully used in this paper to manufacture perfect Ti6Al4V parts. Based on a series of single tracks, the processing windows were firstly proposed, corresponding to different melting mechanisms. And selective laser melted Ti6Al4V parts using various parameters within the processing map were investigated in terms of microstructure, roughness, densification and microhardness. It was found that the microstructure, roughness, densification and microhardness of Ti6Al4V parts were a strong function of processing parameters. An excellent Ti6Al4V part with the high microhardness and the smooth surface can be manufactured by selective laser melting using preferable laser power 110W and scanning speed 0.4m/s, corresponding to continuous melting mechanism. The density is so high that it can be comparable to that of bulk Ti6Al4V alloy.},
  doi        = {10.1016/j.matdes.2011.09.051},
  file       = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271569/1-s2.0-S0261306911X00109/1-s2.0-S0261306911006765/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEIT%2F%2F%2F%2F%2F%2F%2F%2F%2F%2FwEaCXVzLWVhc3QtMSJIMEYCIQDMulorBchlWIw6hFFQn7YKO7CtRrlhnruB6QlpX6tWggIhAMD8xHnevMTWdQq5Qc6q29O5zrx4CUCtXbakcjweU2AuKr0DCO3%2F%2F%2F%2F%2F%2F%2F%2F%2F%2FwEQAxoMMDU5MDAzNTQ2ODY1IgyxlNKHam%2F6clCGI5YqkQPC4Mymo3z92WG3Zq23TDSMSHhIpR%2Bi%2BIkQncdGv2%2B24LApqRTRGXpdrH5U4hsj%2Bmk9h8kI9UVhK%2BVzdz1PvrOpno25rakvwwNM%2BGom5TywUDbaB%2FLUT8iTOnwac6raeHtZU02MSojZ1CWsCN0KrMuNPcJvR3gBS7BRNo4OGNRoaH6%2Bztn6evpQXOInxnyItiaIrm4OjG7zdjsta72ENRSAGcg2pI3o%2FMfV%2F8OP%2Foih2ws2nUK7grcwOcvF1HBdWf%2Bx0su0mQkBfqay%2B2DbdSRdV19ZQwwwopKzR%2BLlsYYO9Ug1aDZG0NB1EA%2FeZUET6UTs1DwCDDxgyML2eJoLaX7RZxi6U4vAUA7%2FESrDkLH2LB%2BNmyU6vRrpUTRjvHwHyzSYimiqqeXQ2mq3cFav3io7iFGKNQNCZWI5sZpMndGA1r8O68srqPNjA5swiC75z%2F8uVz0A9VwklwOoKK5IEPiYxY1lfM5xbYs4nO0NxR2QjnfW9ThVG5z3FYOxJLDeXUhB3PQ8Iyf9SyEpqCcJivgl4DCFkJCEBjrqAYPS30a%2BcnGCwd0o8Rf%2Fy73odaPmGSJTpnXeJRZnTzYPdnYMDUSBzZ9dKVk5NWPSWFEVNZWGR7nc5uF2jgxc6qDUJY%2BpbSIo4cXyy1tG3yNwua8O2a%2B%2FcwKlWbY8NAVeM4SdsuWXer%2BCfTtkwipjmjSDIwOVy%2BEBXHrlETDMkm%2F8Ag0sKigw9bwlHpK6uw%2FtF%2BybzQhiZjiwEPB4F4zhmuU5joB5BJA7iJ9e8bs8dyMXfJVYRVzpF0WhVHU2a%2FNLtLVMGIqp2sUpNrtoY3S19vvJFva%2FePgrRCjNBndAWVWdu5kRepQWCipKFQ%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20210424T130349Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTY6HJRPHF6%2F20210424%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=fb0e5860e11469eedc24868128ebdd649aea2b69ced7498cd1c7fc5a13454ee9&hash=c46bd743c7ec4c0ffeabc66e4292f12284cce91fa559f8d33df41eac8049b946&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S0261306911006765&tid=spdf-a556f811-6354-4e39-8897-f90f687ae958&sid=243cf5922cb291427c89ca152f15e527b6cdgxrqb&type=client:application/pdf},
