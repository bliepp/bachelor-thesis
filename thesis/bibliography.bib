@Article{roth2019imd,
  author   = {Roth, Johannes and Eisfeld, Eugen and Klein, Dominic and Hocker, Stephen and Lipp, Hansjörg and Trebin, Hans-Rainer},
  journal  = {The European Physical Journal Special Topics},
