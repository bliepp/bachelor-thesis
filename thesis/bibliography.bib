@Article{panwisawas2017mesoscale,
  author     = {Panwisawas, Chinnapat and Qiu, Chunlei and Anderson, Magnus J. and Sovani, Yogesh and Turner, Richard P. and Attallah, Moataz M. and Brooks, Jeffery W. and Basoalto, Hector C.},
  journal    = {Computational Materials Science},
  title      = {Mesoscale modelling of selective laser melting: {Thermal} fluid dynamics and microstructural evolution},
  year       = {2017},
  issn       = {0927-0256},
  month      = jan,
  pages      = {479--490},
  volume     = {126},
  abstract   = {In this paper, an integrated computational materials science approach for selective laser melting (SLM) at the mesoscale is presented. A particle dropping model was developed to simulate the representative powder-bed particle distribution of a measured titanium alloy powder. Thermal fluid flow and resulting microstructural evolution of a set of laser scanned single tracks with different powder layer thicknesses and scanning speeds during SLM were also studied using both computational and experimental approaches. The simulated powder particle distribution was found to be consistent with experimental measurement. The thermal fluid flow model predicts that single laser scanned tracks become increasingly irregular-shaped with increased powder layer thickness and increased laser scanning speed. These findings were reinforced by scanning electron microscopy analysis. The more dispersed dissipation of the localised heat for thicker powder layers is understood to cause increased melting and evaporation. This can lead to increased Marangoni force and recoil pressure which in turn destabilises the melt flow. The use of an argon atmosphere speeds up the solidification process when compared with air but does not affect the morphology of single tracks significantly. The predicted microstructure was consistent with the electron backscattered diffraction data. The microstructure-based modelling methodology considering the representative powder size distribution provides a good predictive capability for the laser-powder interaction behaviour, surface structure and porosity development.},
  comment    = {sarah: ergänzend noch interessant bzgl. Fluiddynamik},
  doi        = {10.1016/j.commatsci.2016.10.011},
  file       = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271513/1-s2.0-S0927025616X00134/1-s2.0-S0927025616305079/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEBAaCXVzLWVhc3QtMSJHMEUCIQCwpgA17HH6NWsLDndtm8u9inhDeWZm5dwm6VPKoe2ftwIgKXh2cRSJDd9wAsvhsQeKZqFWw%2B5dPeJb8qYoWepZQNMqtAMIeRADGgwwNTkwMDM1NDY4NjUiDIA6CEmkUOMtLO5e%2ByqRA2GhLJYvILU7oGN%2BYNlsaGlyQBLf1vuRpYBkaFCADk3SiFhbARhGyRC6AqAU1qmukjdKdceB90OGeTQA0DoedRvsfMzLodpTJViRhgGOBCorcHKFwy7qBG8QlFnINc%2BEAZD%2F%2B2SEmJ9dOEFqx937PjuTvlD3w50Phks7hSgGaWa9WDfQAwbqElvuGt6ZwMLjtlGuJnv6cxZb2Pr0wBUnHwDJ02jWsAuelcV1xgj%2FGNwxChsE0%2B%2BNZlSLIzJJ9XBkGcHbCTHzb%2FU6uIaFj72gcQL0qsKnYbkmstxXpigVFxf%2F9AuawrZR80INwZqQAjvS9ak8zHTGcuzlA3HsYUgc29Hhukk9PQsvq%2BgQQMBG2tbolbBUfuPPbxJYs1A5xHgjRmp0IafmRPjkh9sqhI1U8jEz%2FetVX2Lyg0EUIQE1eAKTJEmj4zajFxOG%2F0zYda4B0NKxt0Y8aHwiiJuRY34dHiLWzpRlzo7s3v23uw8LXNEWpiGZXBRFl5h7ftxXLzODIzyFQSf8iI2Kja2aE4Z%2FMRR9MP%2FB9oMGOusBXjRhBPGyPCwaa2sXjIQcMvxb7vwdMJzi9gOLYm4o0xGlZovjKaD3sEo6qwNgAt5aDzn3MQxxsqeOlSrDmynupwKUpatq1eqakaW0pvel5oZfuQUosrdk6DG12LGOCAHTBsAA%2FnZSKKeXmrmQfoaYhmbg0RBVuMTRHdhQSZTmS%2FbxeErdtTh5KlPQDqUmP%2F731odgeXjBw1jMludBjJEavsQqQBInGfF9R%2BUX2BD1ff4yPLTx4MCuF%2FCkgqL4yHgCUiuv19VKSFIRwcQscyEnChrrF1rijEoi6uIFhuueyZ9fdDkXL7SGhz%2B%2Frw%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20210419T164833Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTYRU54RF46%2F20210419%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=82943974d3301566ca1bf5b42fb709e66da8bd8f0dcae3390a7d8a99caf7cb98&hash=60ad246aa7f75106a79e538b7f829e88829f7fcba10376c21265a7e207288b20&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S0927025616305079&tid=spdf-009ec98d-712c-4bde-b782-6b039b7bb84f&sid=d01389fe4f2151415f69564744e86a1cbc5fgxrqb&type=client:application/pdf},
