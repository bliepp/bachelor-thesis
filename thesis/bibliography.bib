  year     = {2019},
  issn     = {1951-6401},
  month    = mar,
  number   = {14},
  pages    = {1831--1836},
  volume   = {227},
  abstract = {IMD is a computer simulation package designed for large-scale simulation studies in materials sciences. IMD can be run with a large number of effective many-body interactions which can be produced from ab-initio calculations. This report is intended to give an overview of the following topics: how to obtain IMD, the design of IMD, capabilities of IMD with special focus on laser ablation, and interaction of IMD with other codes.},
  doi      = {10.1140/epjst/e2019-800147-7},
  file     = {Springer Full Text PDF:https\://link.springer.com/content/pdf/10.1140%2Fepjst%2Fe2019-800147-7.pdf:application/pdf},
  groups   = {Software},
  language = {en},
  url      = {https://doi.org/10.1140/epjst/e2019-800147-7},
  urldate  = {2021-04-19},
}

